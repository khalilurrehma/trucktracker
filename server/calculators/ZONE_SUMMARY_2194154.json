    {
      "calc_id": 2194144,
      "fields": [
        {
          "aggregate": "summary",
          "name": "zone_total_volume_m3"
        }
      ],
      "method": "aggregate",
      "name": "daily_zone_volume_data",
      "type": "calc"
    },
    {
      "expression": "daily_zone_volume_data['zone_total_volume_m3']",
      "name": "zone_material_moved_today",
      "type": "interval"
    },
    {
      "counter": "zone_material_moved_today",
      "name": "zone_total_material_moved_m3",
      "type": "accumulator"
    },
    {
      "name": "day_increment",
      "type": "specified",
      "value": 1
    },
    {
      "counter": "day_increment",
      "name": "total_days",
      "type": "accumulator"
    },
    {
      "expression": "geofence('metadata.zone_bank_volume_m3') * (1 + (geofence('metadata.zone_bank_swell_factor') / 100))",
      "name": "zone_operation_total_m3",
      "type": "interval"
    },
    {
      "expression": "zone_operation_total_m3 - zone_total_material_moved_m3",
      "name": "zone_remaining_material_m3",
      "type": "interval"
    },
    {
      "expression": "zone_total_material_moved_m3 / total_days",
      "name": "zone_avg_daily_volume_m3",
      "type": "interval"
    },
    {
      "expression": "if(zone_avg_daily_volume_m3 > 0, (zone_remaining_material_m3 / zone_avg_daily_volume_m3) * 24, 0)",
